{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "serverless-text-to-speech:SoundFileUrl",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/serverless-text-to-speech/template.yaml",
                "logicalId": "SoundFileUrl"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/aws-dotnet-textract/src/aws-dotnet-textract/bin/Debug/net6.0/aws-dotnet-textract.dll",
            "args": [],
            "cwd": "${workspaceFolder}/aws-dotnet-textract/src/aws-dotnet-textract",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}